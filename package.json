{
    "name": "student-randomizer",
    "version": "1.0.0",
    "license": "MIT",
    "main": "electron.js",
    "scripts": {
        "start": "electron dist/electron.js",
        "build-electron-linux": "ng build --base-href . && cp src/electron/* dist",
        "build-electron-windows": "ng build --base-href . && copy src\\electron\\* dist",
        "electron-linux": "npm run build-electron-linux && electron dist/electron.js",
        "electron-windows": "npm run build-electron-windows && electron dist/electron.js",
        "package-mac": "electron-packager dist --overwrite --platform=darwin --arch=x64 --prune=true --out=release-builds",
        "package-win": "electron-packager /dist Randomizer --platform=win32 --arch=ia32 --out=release-builds",
        "package-linux": "electron-packager dist --overwrite --platform=linux --arch=x64 --prune=true --out=release-builds"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "^5.2.3",
        "@angular/cdk": "^5.1.1",
        "@angular/cli": "^1.7.0",
        "@angular/common": "^5.2.3",
        "@angular/compiler": "^5.2.3",
        "@angular/compiler-cli": "^5.2.3",
        "@angular/core": "^5.2.3",
        "@angular/flex-layout": "^2.0.0-beta.12",
        "@angular/forms": "^5.2.3",
        "@angular/http": "^5.2.3",
        "@angular/material": "^5.1.1",
        "@angular/platform-browser": "^5.2.3",
        "@angular/platform-browser-dynamic": "^5.2.3",
        "@angular/router": "^5.2.3",
        "core-js": "^2.5.3",
        "hammerjs": "^2.0.8",
        "rxjs": "^5.5.6",
        "web-animations-js": "^2.3.1",
        "zone.js": "^0.8.2",
        "font-awesome": "^4.7.0",
        "ngx-electron": "^1.0.4"
    },
    "devDependencies": {
        "@types/cucumber": "0.0.38",
        "@types/jasmine": "2.5.45",
        "@types/node": "^6.0.96",
        "@types/selenium-webdriver": "^3.0.8",
        "chai": "^4.1.0",
        "chai-as-promised": "^7.0.0",
        "codelyzer": "^4.0.1",
        "cucumber": "^2.2.0",
        "cucumber-html-report": "^0.6.3",
        "cucumber-html-reporter": "^2.0.3",
        "electron": "^7.2.4",
        "electron-packager": "^11.0.1",
        "jasmine-core": "~2.6.2",
        "jasmine-spec-reporter": "~4.1.0",
        "karma": "~1.7.0",
        "karma-chrome-launcher": "~2.1.1",
        "karma-cli": "~1.0.1",
        "karma-coverage-istanbul-reporter": "^1.3.3",
        "karma-jasmine": "^1.1.1",
        "karma-jasmine-html-reporter": "^0.2.2",
        "protractor": "~5.1.2",
        "protractor-cucumber-framework": "^3.1.2",
        "ts-node": "^4.1.0",
        "tslint": "^5.9.0",
        "typescript": "^2.4.2"
    }
}
